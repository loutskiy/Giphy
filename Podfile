target 'Giphy' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
  use_frameworks!

  # Pods for Giphy
pod 'GiphyCoreSDK'
pod 'SDWebImage', '~> 4.0'
pod 'SDWebImage/GIF'
end
